{
  "name": "node-typescript-template",
  "private": true,
  "main": "./src/index.ts",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "build": "pnpm genv && tsc",
    "watch": "pnpm genv && tsc-watch --onSuccess \"node ./build/index.js\"",
    "genv": "generate-env-types --definitionPath=\"./src/typings/env.d.ts\" --examplePath=\"\"",
    "gexample": "generate-env-types --definitionPath=\"\"",
    "lint": "eslint --fix . && prettier ./**/**.{json,md} --write"
  },
  "dependencies": {
    "@discordjs/builders": "^0.11.0",
    "@discordjs/rest": "^0.2.0-canary.0",
    "chalk": "^4.1.2",
    "discord-api-types": "^0.26.1",
    "discord.js": "^13.6.0",
    "dotenv": "^11.0.0",
    "mongoose": "^6.1.7",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/node": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "generate-env-types": "git+https://github.com/ryanArora/generate-env-types.git#1.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.5.4"
  },
  "engines": {
    "node": ">=16"
  }
}
